{
	"editor.fontSize": 16,
	"editor.fontWeight": "400",
	"editor.fontFamily": "'Cascadia Code', 'IBM Plex Mono', 'Dank Mono'",
	"editor.renderWhitespace": "none",
	"files.autoSave": "off",
	"explorer.openEditors.visible": 10,
	"terminal.external.osxExec": "iTerm",
	"files.exclude": {
		".cache": true,
		".history/": true,
		".idea/": true,
		".vscode": true,
		".bundle": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/CVS": true,
		"node_modules/": true,
		"vendor/": true
	},
	"editor.renderIndentGuides": false,
	"[javascriptreact]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2
	},
	"window.closeWhenEmpty": true,
	"extensions.ignoreRecommendations": true,
	"editor.fontLigatures": true,
	"workbench.startupEditor": "newUntitledFile",
	"editor.tabCompletion": "on",
	"terminal.integrated.lineHeight": 1.4,
	"emmet.triggerExpansionOnTab": true,
	"editor.multiCursorModifier": "alt",
	"editor.renderControlCharacters": true,
	"editor.showFoldingControls": "always",
	"debug.openDebug": "neverOpen",
	"debug.internalConsoleOptions": "neverOpen",
	"debug.showInStatusBar": "never",
	"breadcrumbs.enabled": false,
	"editor.lineHeight": 28,
	"git.confirmSync": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"window.openFoldersInNewWindow": "on",
	"gitlab.showMrStatusOnStatusBar": false,
	"gitlab.showIssueLinkOnStatusBar": false,
	"editor.tabSize": 2,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.codeActionsOnSave": {
		"source.organizeImports": false,
		"source.fixAll.eslint": true,
		"source.fixAll.stylelint": true
	},
	"breadcrumbs.symbolPath": "off",
	"git.path": "/usr/local/bin/git",
	"workbench.editor.revealIfOpen": true,
	"editor.glyphMargin": true,
	"editor.cursorStyle": "line",
	"editor.cursorBlinking": "phase",
	"gitlens.codeLens.enabled": false,
	"gitlens.statusBar.enabled": false,
	"gitlens.currentLine.enabled": false,
	"workbench.fontAliasing": "auto",
	"terminal.integrated.letterSpacing": 0.5,
	"terminal.integrated.fontWeightBold": "normal",
	"terminal.integrated.fontSize": 14,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.disableLanguages": ["json", "markdown"],
	"editor.formatOnSave": true,
	"spellright.statusBarIndicator": false,
	"gitlab.showStatusBarLinks": false,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true
	},
	"[vue]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"telemetry.enableTelemetry": false,
	"update.mode": "manual",
	"vetur.format.defaultFormatter.html": "none",
	"vetur.format.defaultFormatter.js": "none",
	"vetur.format.defaultFormatter.css": "none",
	"vetur.format.defaultFormatter.less": "none",
	"vetur.format.defaultFormatter.postcss": "none",
	"vetur.format.defaultFormatter.scss": "none",
	"vetur.format.defaultFormatter.stylus": "none",
	"vetur.format.defaultFormatter.ts": "none",
	"vetur.experimental.templateInterpolationService": true,
	"eslint.packageManager": "yarn",
	"eslint.run": "onSave",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"hamlLint.useBundler": true,
	"terminal.integrated.rendererType": "experimentalWebgl",
	"terminal.integrated.fontWeight": "400",
	"vim.enableNeovim": true,
	"files.associations": {
		"*.haml": "haml"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"solargraph.autoformat": true,
	"solargraph.diagnostics": true,
	"solargraph.formatting": true,
	"solargraph.bundlerPath": "/Users/ammar/.asdf/shims/bundle",
	"editor.accessibilitySupport": "off",
	"workbench.settings.editor": "json",
	"workbench.editor.showIcons": false,
	"workbench.statusBar.visible": false,
	"window.newWindowDimensions": "maximized",
	"search.quickOpen.includeHistory": false,
	"workbench.list.horizontalScrolling": true,
	"workbench.activityBar.visible": false,
	"window.autoDetectColorScheme": true,
	"zenMode.fullScreen": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"window.titleBarStyle": "native",
	"customizeUI.titleBar": "inline",
	"customizeUI.activityBar": "wide",
	"workbench.editor.showTabs": true,
	"editor.minimap.enabled": false,
	"editor.snippetSuggestions": "top",
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.gotoLocation.multipleDeclarations": "goto",
	"editor.gotoLocation.multipleImplementations": "goto",
	"editor.gotoLocation.multipleTypeDefinitions": "goto",
	"todohighlight.keywords": ["@TODO"],
	"todohighlight.defaultStyle": {
		"color": "#333",
		"border": "1px solid #ffab00",
		"borderRadius": "2px", //NOTE: using borderRadius along with `border` or you will see nothing change
		"backgroundColor": "#ffab00"
	},
	"editor.gotoLocation.multipleDefinitions": "goto",
	"togglequotes.chars": ["\"", "'", "`"],
	"window.zoomLevel": 0,
	"workbench.sideBar.location": "left",
	"tabnine.experimentalAutoImports": true,
	"vim.useSystemClipboard": true,
	"workbench.iconTheme": "material-icon-theme",
	"explorer.confirmDragAndDrop": false,
	"gitlens.hovers.currentLine.over": "line",
	"explorer.confirmDelete": false,
	"git.autofetch": true,
	"vsicons.dontShowNewVersionMessage": true,
	"git.enableSmartCommit": true,
	"workbench.editor.enablePreview": false,
	"prettier.singleQuote": true,
	"vim.leader": ",",
	"vim.camelCaseMotion.enable": true,
	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useCtrlKeys": true,
	"vim.handleKeys": {
		"<C-d>": false,
		"<C-r>": true
	},

	"vetur.validation.interpolation": false,
	"workbench.colorTheme": "Palenight Theme",
	"editor.linkedEditing": true,
	"kite.showWelcomeNotificationOnStartup": false,
	"gitlens.advanced.messages": {
		"suppressCommitNotFoundWarning": true
	},
	"typescript.disableAutomaticTypeAcquisition": false
}
